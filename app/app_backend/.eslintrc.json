{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "import/no-dynamic-require": 0,
        "no-path-concat": 0,
        "prefer-template": 0,
        "global-require": 0,
        "no-console": 0
    }
}
